    "@types/number-to-words": "^1.2.3",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^7.3.0"
  }
}
